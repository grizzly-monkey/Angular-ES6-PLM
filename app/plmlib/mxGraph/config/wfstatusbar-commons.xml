	<add as="fit" action="fit" icon="plmlib/mxGraph/images/icons/centerzoom_16.png"/>
	<add as="zoomIn" action="zoomIn" icon="plmlib/mxGraph/images/icons/zoomin_16.png"/>
	<add as="zoomCustom" action="zoomCustom"/>
	<add as="zoomOut" action="zoomOut" icon="plmlib/mxGraph/images/icons/zoomout_16.png"/>
	<separator/>
	<add as="layout" action="toggleOutline" icon="plmlib/mxGraph/images/icons/workflow_16.png" showText="true"/>
